{
    "configurations": [
         {
            "name": "Arm GCC (Release)",
            "includePath": [
                "${workspaceFolder}/modm/ext",
                "${workspaceFolder}/modm/ext/cmsis/core",
                "${workspaceFolder}/modm/ext/cmsis/device",
                "${workspaceFolder}/modm/ext/gcc",
                "${workspaceFolder}/modm/src"
            ],
            "defines": [
                "STM32G4"
            ],
            "compilerPath": "/opt/homebrew/bin/arm-none-eabi-g++",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16",
                "-mthumb"
            ],
            "cStandard": "gnu23",
            "cppStandard": "c++23",
            "intelliSenseMode": "gcc-arm"
        },
         {
            "name": "Arm GCC (Debug)",
            "includePath": [
                "${workspaceFolder}/modm/ext",
                "${workspaceFolder}/modm/ext/cmsis/core",
                "${workspaceFolder}/modm/ext/cmsis/device",
                "${workspaceFolder}/modm/ext/gcc",
                "${workspaceFolder}/modm/src"
            ],
            "defines": [
                "MODM_DEBUG_BUILD",
                "STM32G4"
            ],
            "compilerPath": "/opt/homebrew/bin/arm-none-eabi-g++",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16",
                "-mthumb"
            ],
            "cStandard": "gnu23",
            "cppStandard": "c++23",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}